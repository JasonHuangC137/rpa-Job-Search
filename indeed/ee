    //如果沒有右下浮動活動內容，則不顯示close_btn
    if ($("#HomeEvent .Event_22y1w_btn").html() == undefined) {
        $(".Event_22y1w_close_btn").hide()
    }

    $('.chevron-up-icon').hide();

    $('.chevron-down-icon').on("click", function () {
        $('.chevron-up-icon').show();
        $('.chevron-down-icon').hide();
        $('.keyword_selected_items_group').show();
    })
    $('.chevron-up-icon').on("click", function () {
        $('.chevron-down-icon').show();
        $('.chevron-up-icon').hide();
        $('.keyword_selected_items_group').hide();
    })

