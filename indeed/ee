<script>
    $(document).ready(function () {
        // 搜尋欄
        $(".UISearchbarInput")
            .click(function () {
                if ($(this).val() == '')
                    $('.UI_searchbar_dropdown').addClass('show');
                else
                    $('.UI_searchbar_dropdown').removeClass('show');

                if (typeof UISearchbarInputEvent === 'function')
                    UISearchbarInputEvent(this, 'click');
            })
            .bind("input propertychange", function (event) {
                $('.UI_searchbar_dropdown').removeClass('show');
                //$('.UI_searchbar_suggest').addClass('show');

                if (typeof UISearchbarInputEvent === 'function')
                    UISearchbarInputEvent(this, 'input propertychange');
            })
            .blur(function (e) {
                e.preventDefault();
                $('.UI_searchbar_dropdown').removeClass('show');
                //$('.UI_searchbar_suggest').removeClass('show');

                if (typeof UISearchbarInputEvent === 'function')
                    UISearchbarInputEvent(this, 'blur');
            });

        $(".UI_searchbar_dropdown").mouseover(function () {
            $('#ks').unbind('blur');
        }).mouseout(function () {
            $('#ks').on('blur', function () { $('.UI_searchbar_dropdown').removeClass('show'); });
        });
    });
</script>
